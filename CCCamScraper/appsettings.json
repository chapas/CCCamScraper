{
  "QuartzJobs": {
    "CCCamScraperJobs": [
      {
        "Name": "freeclinePrint",
        "URLToScrape": "https://global-satinfo.in/viewtopic.php?t=91&st=1&sd=d&view=print",
        "ScrapePath": "div.entry div",
        "Schedule": "0 0 */6 * * ?",
        "RandomnessInMinutes": 33,
        "RunOnceAtStartUp": true,
        "Enabled": true
      },
      {
        "Name": "satforumPrint",
        "URLToScrape": "https://sat-forum.net/viewtopic.php?t=128&st=1&sd=d&view=print",
        "ScrapePath": "div.entry div",
        "Schedule": "0 0 */6 * * ?",
        "RandomnessInMinutes": 41,
        "RunOnceAtStartUp": true,
        "Enabled": true
      },
      {
        "Name": "testious",
        "URLToScrape": "https:///testious.com/old-free-cccam-servers/<YYYY>-<MM>-<DD>/",
        "ScrapePath": "div.entry div",
        "Schedule": "0 0 */6 * * ?",
        "RandomnessInMinutes": 37,
        "RunOnceAtStartUp": true,
        "Enabled": true
      },
      {
        "Name": "cccamsate",
        "URLToScrape": "https://cccamsate.com/free",
        "ScrapePath": "div.entry div",
        "Schedule": "0 0 */12 * * ?",
        "RandomnessInMinutes": 17,
        "RunOnceAtStartUp": true,
        "Enabled": true
      },
      {
        "Name": "bosscccam",
        "URLToScrape": "https://bosscccam.co/Test.php",
        "ScrapePath": "div.entry div",
        "Schedule": "0 0 */12 * * ?",
        "RandomnessInMinutes": 11,
        "RunOnceAtStartUp": true,
        "Enabled": true
      },
      {
        "Name": "RemoveReadersWithoutUserDefinedCaidJob",
        "Schedule": "0 0 */6 * * ? *",
        "RunOnceAtStartUp": false,
        "Enabled": false
      },
      {
        "Name": "RemoveReadersWithUnwantedStatusJob",
        "Schedule": "0 0 */6 * * ? *",
        "RunOnceAtStartUp": false,
        "Enabled": false
      },
      {
        "Name": "RemoveReadersWithECMNotOKJob",
        "Schedule": "0 0 */6 * * ? *",
        "RunOnceAtStartUp": false,
        "Enabled": true
      }
    ]
  },
  "OsCam": {
    "FlareSolverrUrl": "http://192.168.1.1:8191/v1",
    "OscamServerPath": "config//oscam//oscam.server",
    "NumberOfBackupsToKeep": 3,
    "OsCamStatusPageURL": "http://192.168.1.1:8888/status.html",
    "OsCamEntitlementsPageURL": "http://192.168.1.1:8888/entitlements.html",
    "OsCamReadersPageURL": "http://192.168.1.1:8888/readers.html",
    "OsCamReaderAPIURL": "http://192.168.1.1:8888/oscamapi.html",
    "CAIDs": [ "1814", "1802" ],
    "ExcludedFromDeletion": [ "egygold.in", "egygold.out", "premiumvip.sytes.net" ],
    "UnwantedStatus": [ "ERROR", "OFF", "UNKNOWN" ],
    "EcmOkThreshold": 0,
    "EcmNokThreshold": 20
  },
  "Serilog": {
    "Using": [
      "Serilog.Enrichers.Environment",
      "Serilog.Enrichers.Process",
      "Serilog.Sinks.Async",
      "Serilog.Sinks.File",
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ],
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "File",
              "Args": {
                "path": "CCCamScraper.log",
                "rollingInterval": "Day",
                "rollOnFileSizeLimit": "true",
                "fileSizeLimitBytes": "20971520",
                "retainedFileCountLimit": "5",
                "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.ffffff zzz} {MachineName} {ProcessId} [{Level:u3}] {SourceContext}: {Message} {Scope}{NewLine}{Exception}"
              }
            },
            {
              "Name": "Console",
              "Args": {
                "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
                "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.ffffff zzz} {MachineName} {ProcessId} [{Level:u3}] {SourceContext}: {Message} {Scope}{NewLine}{Exception}"
              }
            }
          ]
        }
      }
    ]
  }
}